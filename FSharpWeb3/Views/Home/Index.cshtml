@model IEnumerable<Library1.Models.Story>


<div class="container">
    <div class="row main-wrapper">
        <div class="col-md-12">
            @foreach (var x in Model)
            {
                <div class="row home-list-item">
                    <div class="panel panel-danger">
                        <div class="panel-heading">
                            <a class="panel-title">@x.title</a>
                        </div>
                        <div class="panel-body">
                            <p>@x.body</p>
                            <p>

                            </p>
                            <a href="~/Story/edit/@x.id"  class="btn btn-default">Edit</a>
                        </div>

                    </div>
                </div>
            }
        </div>
    </div>
</div>