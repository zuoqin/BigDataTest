@model IEnumerable<Library1.Models.Story>

    <div ng-app="BigData" class="row">
        <input type="text" ng-model="models.helloAngular" />
        <h1>{{models.helloAngular}}</h1>
    </div>

    <div class="container">
        <div class="row main-wrapper">
            <div class="col-md-12">
                @foreach (var x in Model)
            {
                    <div class="row home-list-item">
                        <div class="panel panel-danger">
                            <div class="panel-heading">
                                <div class="row">
                                    <div class="col-md-6">
                                        <a class="panel-title">@x.title</a>
                                    </div>
                                    <div class="col-md-6">
                                        @Html.ActionLink("Delete", "Delete", "Story", new { id = @x.id }, new { @class = "hkj" })

                                    </div>


                                </div>
                            </div>
                            <div class="panel-body">
                                <p>@x.body</p>
                                <p>

                                </p>
                                <a href="~/Story/edit/@x.id" class="btn btn-default">Edit</a>
                            </div>

                        </div>
                    </div>
                }
            </div>
        </div>
    </div>
